   "source": [
    "# 要求使用空间复杂度为 O(1) 的 原地 算法。\n",
    "nums = [1,2,3,4,5,6,7]\n",
    "k = 3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def rotate(nums, k):\n",
    "    lenth = len(nums)\n",
    "    nums[:] = nums[lenth-k:]+nums[:lenth-k]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "rotate(nums, k)"
   ]
  },
